class Concatenate
    static const tensor_order := 1
    property is_a_vector
    property is_a_Concatenate_vector
    
    lists ~= VarArgs
    def init( va ) when not va.is_a_Concatenate_vector
        lists.init( va )
    def destroy
        lists.destroy()
    def get_size
        res := SizeType( 0 )
        for l in lists
            res += l.size
        return res # sum( lists.map( _0.size ) )
    def get_values
        return self
    def map( function ) 
        res := Concatenate()
        res.lists.init()
        for l in lists
            res.lists.push_unnamed( l.map(function) )
        return res
    def select( n ) when n.tensor_order <= 0
        nc := n
        for l in lists
            if nc < l.size
                return l[ nc ]
            nc -= l.size
    def __for__( block )
        for l in lists
            for v in l
                block( v )
    def get_sub_types
        res := VarArgs()
        for l in lists
            for s in l.sub_types
                res.push_unnamed( s )
        return res
                
