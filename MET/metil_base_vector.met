class CombinatorialList
    n := 0
    s := 0
    def init( nb_values, list_size )
        n.init( nb_values )
        s.init( list_size )
    def __for__( block )
        if s >= n
            it := Vec[Int32]( 0 .. n )
            while true
                block(it)
                for i in 0 .. n
                    if i == n-1
                        it[i]++
                        if it[i] == s
                            return None
                        break
                    if it[i]+1 < it[i+1]
                        it[i]++
